//This is the practice set of day 2.
import java.util.Scanner;

class Main {
  public static void main(String[] args) {
    System.out.println("ENTER QUESTION NUMBER FROM 1 TO 5");
    Scanner sc = new Scanner(System.in);
    int question = sc.nextInt();
    switch (question)
      {
        case 1:
          // Question1
          float a = 7/4.0f * 9/2.0f;
          System.out.println(a);
          break;
        case 2:
          // Question2
          char grade = 'B';
          grade = (char)(grade + 8);
          System.out.println(grade);
          // Decrypting the grade
          grade = (char)(grade - 8);
          System.out.println(grade);
          break;
        case 3:
          // Question3
          System.out.print("Enter a number: ");
          double userEnteredNumber = sc.nextDouble();

          double givenNumber = 42.0;

          if (givenNumber > userEnteredNumber) {
              System.out.println(givenNumber + " is greater than the entered number.");
          } else {
              System.out.println(givenNumber + " is not greater than the entered number.");
          }
                 break;
        case 4:
          // Question4
          System.out.print("Enter the value of v: ");
          double v = sc.nextDouble();

          System.out.print("Enter the value of u: ");
          double u = sc.nextDouble();

          System.out.print("Enter the value of a: ");
          double a = sc.nextDouble();

          System.out.print("Enter the value of s: ");
          double s = sc.nextDouble();

          double result = (Math.pow(v, 2) - Math.pow(u, 2)) / (2 * a * s);

          System.out.println("Result: " + result);
          break;
        case 5:
          // Question5
           int a = sc.nextInt();
            System.out.println(a>8);
            System.out.println(7*49/7+35/7);
          break;

        default:
          System.out.println("Invalid Input");
          break;
      }
  
  }
}
